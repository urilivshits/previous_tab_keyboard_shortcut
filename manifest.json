{
  "manifest_version": 3,
  "name": "Previous Tab Keyboard Shortcut",
  "version": "1.0.0",
  "description": "Switch to your previously focused tab with a keyboard shortcut. Works across multiple windows and survives browser restarts.",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "switch-to-previous-tab": {
      "suggested_key": {
        "default": "Alt+Q"
      },
      "description": "Switch to previously focused tab"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "urilivshits",
  "homepage_url": "https://github.com/urilivshits/previous_tab_keyboard_shortcut"
}
